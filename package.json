  "scripts": {
    "build:dev": "webpack --env.config=base,app,inline-css --env.env=development",
    "build:dev:dashboard": "webpack-dashboard --env.config=base,app,inline-css,dashboard --env.env=development",
    "build:prod": "webpack --env.config=base,app,extract-css --env.env=production",
    "build:prod:uglify": "webpack -p --env.config=base,app,extract-css --env.env=production",
    "build:prod:uglify:dashboard": "webpack-dashboard -- webpack -p --env.config=base,apract-css,dashboard --env.env=production",
    "wds": "webpack-dev-server --hot --env.config=base,app,inline-css,wds --env.env=development",
    "wds:dashboard": "webpack-dashboard -- webpack-dev-server --hot --env.config=base,app,inline-css,wds,dashboard --env.env=development",
    "deploy": "cross-env GH_PAGES=true npm run build:prod:uglify && gh-pages -d dist",
    "server": "static-server -p 8090 dist"
  },
  "dependencies": {
    "bootstrap": "4.0.0-alpha.6",
    "jquery": "^3.2.1",
    "tether": "^1.4.0",
